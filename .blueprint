models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string:100 nullable' }
    Post: { title: 'string:100', slug: 'string:150', content: longtext, thumbnail: 'nullable string:255', user_id: 'id unsigned foreign nullable', category_id: 'id unsigned foreign nullable', published_at: 'nullable timestamp', timestamps: timestamps, relationships: { belongsTo: User, hasMany: Category } }
    Category: { cat_name: 'string:150', slug: 'string:150', timestamps: timestamps, relationships: { belongsToMany: Post } }
    Team: { team_name: 'string:150', thumbnail: 'nullable string:255', social1: 'nullable string:150', social2: 'nullable string:150', social3: 'nullable string:150', departament_id: 'id unsigned foreign nullable', timestamps: timestamps, relationships: { belongsToMany: Departament } }
    Departament: { dep_name: 'string:150', timestamps: timestamps, relationships: { hasMany: Team } }
    Customer: { cus_name: 'string:150', slug: 'string:150', thumbnail: 'nullable string:255', since: 'nullable timestamp', content: longtext, results: longtext, url: 'string:150', timestamps: timestamps, service_id: 'id unsigned foreign nullable', relationships: { hasMany: Project, belongsToMany: Service } }
    Project: { title: 'string:150', description: 'string:255', slug: 'string:150', content: longtext, thumbnail: 'string:255', images: longtext, customer_id: 'id unsigned foreign nullable', service_id: 'id unsigned foreign', timestamps: timestamps, relationships: { belongsTo: Customer, hasMany: Service } }
    Service: { name: 'string:150', timestamps: timestamps, relationships: { hasMany: 'Project, Customer' } }
